{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "dev": "rimraf wwwroot/ && node scripts/dev.js",
    "start": "react-scripts-ts start",
    "dtos": "cd src && typescript-ref",
    "build": "rimraf wwwroot/ && react-scripts-ts build && (move build wwwroot || mv build wwwroot)",
    "publish": "npm run build",
    "test": "react-scripts-ts test --watchAll",
    "test-coverage": "npm test -- --coverage",
    "eject": "react-scripts-ts eject"
  },
  "proxy": "http://localhost:5000/",
  "dependencies": {
    "@servicestack/client": "^1.0.15",
    "bootstrap": "^4.2.1",
    "classnames": "^2.2.6",
    "font-awesome": "^4.7.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^3.1.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "gulp": "^4.0.0",
    "react-test-renderer": "^16.7.0",
    "typescript": "^3.2.4"
  }
}
